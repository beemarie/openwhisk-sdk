{
  "name": "openwhisk-sdk",
  "version": "0.0.1",
  "description": "Openwhisk SDK",
  "main": "index.js",
  "scripts": {
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "unset TEST_OPENWHISK && nyc mocha test/actions/*",
    "test-integration": "export TEST_OPENWHISK=true && mocha --timeout 50000 test/actions/*",
    "lint": "eslint .",
    "autofix": "eslint --fix ."
  },
  "keywords": [
    "openwhisk",
    "watson",
    "ibm-watson"
  ],
  "author": "IBM Corp",
  "license": "MIT",
  "dependencies": {
    "json-loader": "~0.5.7",
    "watson-developer-cloud": "~3.0.4",
    "webpack": "~3.10.0"
  },
  "devDependencies": {
    "eslint": "~4.7.2",
    "eslint-config-airbnb-base": "~12.0.2",
    "eslint-plugin-import": "~2.7.0",
    "mocha": "~3.5.3",
    "nock": "~9.0.21",
    "nyc": "~11.3.0",
    "object.omit": "~3.0.0",
    "openwhisk": "~3.11.0",
    "prettier-eslint": "~8.1.1",
    "extend": "~3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.ibm.com:Watson/openwhisk-sdk.git"
  }
}
