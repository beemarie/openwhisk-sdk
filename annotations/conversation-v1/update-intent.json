{
  "description": "Update intent.",
  "parameters": [
    {
      "name": "username",
      "required": false,
      "bindTime": true,
      "description": "The service username"
    },
    {
      "name": "password",
      "required": false,
      "bindTime": true,
      "description": "The service password"
    },
    {
      "name": "headers",
      "required": false,
      "bindTime": false,
      "description": "The request headers"
    },
    {
      "name": "headers.X-Watson-Learning-Opt-Out",
      "required": false,
      "bindTime": false,
      "description": "opt-out of data collection"
    },
    {
      "name": "url",
      "required": false,
      "bindTime": false,
      "description": "override default service base url"
    },
    {
      "name": "version_date",
      "required": true,
      "bindTime": false,
      "description": "Release date of the API version in YYYY-MM-DD format."
    },
    {
      "name": "workspace_id",
      "required": true,
      "bindTime": false,
      "description": "The workspace ID."
    },
    {
      "name": "intent",
      "required": true,
      "bindTime": false,
      "description": "The intent name (for example, `pizza_order`)."
    },
    {
      "name": "new_intent",
      "required": false,
      "bindTime": false,
      "description": "The name of the intent."
    },
    {
      "name": "new_description",
      "required": false,
      "bindTime": false,
      "description": "The description of the intent."
    },
    {
      "name": "new_examples",
      "required": false,
      "bindTime": false,
      "description": "An array of user input examples for the intent."
    }
  ],
  "sampleInput": {
    "username": "XXX",
    "password": "XXX",
    "version_date": "2016-10-19",
    "workspace_id": "{workspace_id}",
    "intent": "hello",
    "new_intent": "hello",
    "new_description": "Updated intent",
    "new_examples": [
      {
        "text": "Good afternoon"
      }
    ]
  },
  "sampleOutput": {
    "intent": "hello",
    "created": "2017-02-16T22:08:13.352Z",
    "updated": "2017-02-16T22:41:45.400Z",
    "description": "Updated intent"
  }
}
