{
  "description": "Find similar images - beta.",
  "parameters": [
    {
      "name": "username",
      "required": false,
      "bindTime": true,
      "description": "The service username"
    },
    {
      "name": "password",
      "required": false,
      "bindTime": true,
      "description": "The service password"
    },
    {
      "name": "headers",
      "required": false,
      "bindTime": false,
      "description": "The request headers"
    },
    {
      "name": "headers.X-Watson-Learning-Opt-Out",
      "required": false,
      "bindTime": false,
      "description": "opt-out of data collection"
    },
    {
      "name": "url",
      "required": false,
      "bindTime": false,
      "description": "override default service base url"
    },
    {
      "name": "version_date",
      "required": true,
      "bindTime": false,
      "description": "Release date of the API version in YYYY-MM-DD format."
    },
    {
      "name": "collection_id",
      "required": true,
      "bindTime": false,
      "description": "The ID of your collection."
    },
    {
      "name": "image_file",
      "required": true,
      "bindTime": false,
      "description":
        "The image file (.jpg or .png) of the image to search against the collection."
    },
    {
      "name": "limit",
      "required": false,
      "bindTime": false,
      "description":
        "The number of similar results you want returned. Default limit is 10 results, you can specify a maximum limit of 100 results."
    },
    {
      "name": "image_file_content_type",
      "required": false,
      "bindTime": false,
      "description": "The content type of image_file."
    }
  ],
  "sampleInput": {
    "username": "XXX",
    "password": "XXX",
    "version_date": "2016-10-19"
  },
  "sampleOutput": {}
}
