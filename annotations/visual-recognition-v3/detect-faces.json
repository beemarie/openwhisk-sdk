{
  "description": "Detect faces in an image.",
  "parameters": [
    {
      "name": "username",
      "required": false,
      "bindTime": true,
      "description": "The service username"
    },
    {
      "name": "password",
      "required": false,
      "bindTime": true,
      "description": "The service password"
    },
    {
      "name": "headers",
      "required": false,
      "bindTime": false,
      "description": "The request headers"
    },
    {
      "name": "headers.X-Watson-Learning-Opt-Out",
      "required": false,
      "bindTime": false,
      "description": "opt-out of data collection"
    },
    {
      "name": "url",
      "required": false,
      "bindTime": false,
      "description": "override default service base url"
    },
    {
      "name": "version_date",
      "required": true,
      "bindTime": false,
      "description": "Release date of the API version in YYYY-MM-DD format."
    },
    {
      "name": "images_file",
      "required": false,
      "bindTime": false,
      "description":
        "An image file (.jpg, .png) or .zip file with images. Include no more than 15 images. You can also include images with the `url` property in the **parameters** object.  All faces are detected, but if there are more than 10 faces in an image, age and gender confidence scores might return scores of 0."
    },
    {
      "name": "parameters",
      "required": false,
      "bindTime": false,
      "description":
        "A JSON string containing the image URL to analyze.   For example: {\"url\": \"...\"}."
    },
    {
      "name": "images_file_content_type",
      "required": false,
      "bindTime": false,
      "description": "The content type of images_file."
    }
  ],
  "sampleInput": {
    "username": "XXX",
    "password": "XXX",
    "version_date": "2016-10-19",
    "images_file": "fs.createReadStream('./resources/obama.jpg')"
  },
  "sampleOutput": {
    "images": [
      {
        "faces": [
          {
            "age": {
              "max": 44,
              "min": 35,
              "score": 0.446989
            },
            "face_location": {
              "height": 159,
              "left": 256,
              "top": 64,
              "width": 92
            },
            "gender": {
              "gender": "MALE",
              "score": 0.99593
            },
            "identity": {
              "name": "Barack Obama",
              "score": 0.970688,
              "type_hierarchy": "/people/politicians/democrats/barack obama"
            }
          }
        ],
        "image": "prez.jpg"
      }
    ],
    "images_processed": 1
  }
}
