{
  "description": "Create a collection.",
  "parameters": [
    {
      "name": "username",
      "required": false,
      "bindTime": true,
      "description": "The service username"
    },
    {
      "name": "password",
      "required": false,
      "bindTime": true,
      "description": "The service password"
    },
    {
      "name": "headers",
      "required": false,
      "bindTime": true,
      "description": "The request headers"
    },
    {
      "name": "headers[X-Watson-Learning-Opt-Out]",
      "required": false,
      "bindTime": true,
      "description": "opt-out of data collection"
    },
    {
      "name": "url",
      "required": false,
      "bindTime": true,
      "description": "override default service base url"
    },
    {
      "name": "version_date",
      "required": true,
      "bindTime": true,
      "description": "Release date of the API version in YYYY-MM-DD format."
    },
    {
      "name": "environment_id",
      "required": true,
      "bindTime": false,
      "description": "The ID of the environment."
    },
    {
      "name": "name",
      "required": true,
      "bindTime": false,
      "description": "The name of the collection to be created."
    },
    {
      "name": "description",
      "required": false,
      "bindTime": false,
      "description": "A description of the collection."
    },
    {
      "name": "configuration_id",
      "required": false,
      "bindTime": false,
      "description":
        "The ID of the configuration in which the collection is to be created."
    },
    {
      "name": "language",
      "required": false,
      "bindTime": false,
      "description":
        "The language of the documents stored in the collection, in the form of an ISO 639-1 language code."
    }
  ],
  "sampleInput": "Coming soon!",
  "sampleOutput": "Coming soon!"
}
