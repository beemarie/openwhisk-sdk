{
  "description": "Update a configuration.",
  "parameters": [
    {
      "name": "username",
      "required": false,
      "bindTime": true,
      "description": "The service username"
    },
    {
      "name": "password",
      "required": false,
      "bindTime": true,
      "description": "The service password"
    },
    {
      "name": "headers",
      "required": false,
      "bindTime": false,
      "description": "The request headers"
    },
    {
      "name": "headers.X-Watson-Learning-Opt-Out",
      "required": false,
      "bindTime": false,
      "description": "opt-out of data collection"
    },
    {
      "name": "url",
      "required": false,
      "bindTime": false,
      "description": "override default service base url"
    },
    {
      "name": "version_date",
      "required": true,
      "bindTime": false,
      "description": "Release date of the API version in YYYY-MM-DD format."
    },
    {
      "name": "environment_id",
      "required": true,
      "bindTime": false,
      "description": "The ID of the environment."
    },
    {
      "name": "configuration_id",
      "required": true,
      "bindTime": false,
      "description": "The ID of the configuration."
    },
    {
      "name": "name",
      "required": true,
      "bindTime": false,
      "description": "The name of the configuration."
    },
    {
      "name": "description",
      "required": false,
      "bindTime": false,
      "description": "The description of the configuration, if available."
    },
    {
      "name": "conversions",
      "required": false,
      "bindTime": false,
      "description": "The document conversion settings for the configuration."
    },
    {
      "name": "enrichments",
      "required": false,
      "bindTime": false,
      "description":
        "An array of document enrichment settings for the configuration."
    },
    {
      "name": "normalizations",
      "required": false,
      "bindTime": false,
      "description":
        "Defines operations that can be used to transform the final output JSON into a normalized form. Operations are executed in the order that they appear in the array."
    }
  ],
  "sampleInput": {
    "username": "XXX",
    "password": "XXX",
    "version_date": "2016-10-19"
  },
  "sampleOutput": {}
}
