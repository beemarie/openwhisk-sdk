{
  "description": "Lists all languages that can be identified by the API.",
  "parameters": [
    {
      "name": "username",
      "required": false,
      "bindTime": true,
      "description": "The service username"
    },
    {
      "name": "password",
      "required": false,
      "bindTime": true,
      "description": "The service password"
    },
    {
      "name": "headers",
      "required": false,
      "bindTime": false,
      "description": "The request headers"
    },
    {
      "name": "headers.X-Watson-Learning-Opt-Out",
      "required": false,
      "bindTime": false,
      "description": "opt-out of data collection"
    },
    {
      "name": "url",
      "required": false,
      "bindTime": false,
      "description": "override default service base url"
    }
  ],
  "sampleInput": {
    "username": "XXX",
    "password": "XXX",
    "version_date": "2016-10-19"
  },
  "sampleOutput": {
    "languages": [
      {
        "language": "af",
        "name": "Afrikaans"
      },
      {
        "language": "ar",
        "name": "Arabic"
      },
      {
        "language": "az",
        "name": "Azerbaijani"
      },
      {
        "language": "ba",
        "name": "Bashkir"
      }
      //...
    ]
  }
}
